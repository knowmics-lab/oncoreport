{
  "name": "oncoreport",
  "productName": "Oncoreport",
  "description": "A pipeline for the annotation and clinical reporting of DNA-seq data",
  "homepage": "https://oncoreport.atlas.dmi.unict.it",
  "bugs": {
    "url": "https://github.com/knowmics-lab/oncoreport/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/knowmics-lab/oncoreport.git"
  },
  "license": "The OncoReport License - https://github.com/knowmics-lab/oncoreport/blob/master/LICENSE",
  "author": {
    "name": "Salvatore Alaimo",
    "email": "salvatore.alaimo@unict.it",
    "url": "https://www.medclin.unict.it/docenti/salvatore.alaimo"
  },
  "contributors": [
    {
      "name": "Grete Francesca Privitera",
      "email": "grete.privitera@unict.it",
      "url": "https://www.medclin.unict.it/assegnisti-di-ricerca/grete.francesca..privitera"
    },
    {
      "name": "Giovanni Micale",
      "email": "gmicale@dmi.unict.it",
      "url": "https://www.medclin.unict.it/docenti/giovanni.micale"
    }
  ],
  "main": "./src/main/main.ts",
  "scripts": {
    "build": "concurrently \"npm run build:main\" \"npm run build:renderer\"",
    "build:dll": "cross-env NODE_ENV=development TS_NODE_TRANSPILE_ONLY=true webpack --config ./.erb/configs/webpack.config.renderer.dev.dll.ts",
    "build:main": "cross-env NODE_ENV=production TS_NODE_TRANSPILE_ONLY=true webpack --config ./.erb/configs/webpack.config.main.prod.ts",
    "build:renderer": "cross-env NODE_ENV=production TS_NODE_TRANSPILE_ONLY=true webpack --config ./.erb/configs/webpack.config.renderer.prod.ts",
    "postinstall": "ts-node .erb/scripts/check-native-dep.js && electron-builder install-app-deps && npm run build:dll",
    "lint": "cross-env NODE_ENV=development eslint . --ext .js,.jsx,.ts,.tsx",
    "package": "ts-node ./.erb/scripts/clean.js dist && npm run build && electron-builder build -wml --x64 --arm64 --publish never && npm run build:dll",
    "release": "ts-node ./.erb/scripts/clean.js dist && npm run build && electron-builder build -wml --x64 --arm64 --publish always && npm run build:dll",
    "rebuild": "electron-rebuild --parallel --types prod,dev,optional --module-dir release/app",
    "start": "ts-node ./.erb/scripts/check-port-in-use.js && npm run start:renderer",
    "start:main": "cross-env NODE_ENV=development electronmon -r ts-node/register/transpile-only .",
    "start:preload": "cross-env NODE_ENV=development TS_NODE_TRANSPILE_ONLY=true webpack --config ./.erb/configs/webpack.config.preload.dev.ts",
    "start:renderer": "cross-env NODE_ENV=development TS_NODE_TRANSPILE_ONLY=true webpack serve --config ./.erb/configs/webpack.config.renderer.dev.ts",
    "test": "jest"
  },
  "browserslist": [],
  "prettier": {
    "singleQuote": true,
    "overrides": [
      {
        "files": [
          ".prettierrc",
          ".eslintrc"
        ],
        "options": {
          "parser": "json"
        }
      }
    ]
  },
  "jest": {
    "moduleDirectories": [
      "node_modules",
      "release/app/node_modules",
      "src"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx",
      "json"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/.erb/mocks/fileMock.js",
      "\\.(css|less|sass|scss)$": "identity-obj-proxy"
    },
    "setupFiles": [
      "./.erb/scripts/check-build-exists.ts"
    ],
    "testEnvironment": "jsdom",
    "testEnvironmentOptions": {
      "url": "http://localhost/"
    },
    "testPathIgnorePatterns": [
      "release/app/dist",
      ".erb/dll"
    ],
    "transform": {
      "\\.(ts|tsx|js|jsx)$": "ts-jest"
    }
  },
  "dependencies": {
    "@date-io/date-fns": "^2.17.0",
    "@electron/remote": "^2.1.1",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@fontsource/roboto": "^5.0.8",
    "@fortawesome/fontawesome-free": "^6.5.1",
    "@mui-treasury/layout": "^5.0.0",
    "@mui/icons-material": "^5.15.3",
    "@mui/lab": "5.0.0-alpha.159",
    "@mui/material": "^5.15.3",
    "@mui/x-date-pickers": "^6.18.7",
    "ansi-to-html": "^0.7.2",
    "axios": "^1.6.4",
    "byte-size": "^8.1.1",
    "check-internet-connected": "^2.0.6",
    "clsx": "^2.1.0",
    "cp-file": "^9.1.0",
    "cpy": "^11.0.0",
    "dayjs": "^1.11.10",
    "dockerode": "^4.0.2",
    "electron-debug": "^3.2.0",
    "electron-dl": "^3.5.1",
    "electron-log": "^4.4.8",
    "electron-store": "^8.1.0",
    "electron-updater": "^6.1.4",
    "electron-util": "^0.17.2",
    "file-type": "^18.7.0",
    "find-free-port": "^2.0.0",
    "formik": "^2.4.5",
    "formik-mui": "5.0.0-alpha.0",
    "formik-mui-lab": "^1.0.0",
    "fs-extra": "^11.2.0",
    "history": "^5.3.0",
    "immer": "^10.0.3",
    "immutable": "5.0.0-beta.4",
    "lodash": "^4.17.21",
    "material-ui-popup-state": "^5.0.10",
    "mimetype-to-fontawesome": "^2.1.0",
    "moment": "^2.30.1",
    "notistack": "^3.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.16.0",
    "react-select": "^5.8.0",
    "reflect-metadata": "^0.2.1",
    "source-map-support": "^0.5.21",
    "sprintf-js": "^1.1.3",
    "ts-debounce": "^4.0.0",
    "tsyringe": "^4.8.0",
    "tus-js-client": "^4.0.1",
    "uniqid": "^5.4.0",
    "use-debounce": "^10.0.0",
    "yup": "^1.3.3"
  },
  "devDependencies": {
    "@electron/notarize": "^2.1.0",
    "@electron/rebuild": "^3.3.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.11",
    "@svgr/webpack": "^8.1.0",
    "@teamsupercell/typings-for-css-modules-loader": "^2.5.2",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@types/dockerode": "^3.3.23",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.5",
    "@types/lodash": "^4.14.202",
    "@types/node": "20.6.2",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@types/react-test-renderer": "^18.0.1",
    "@types/sprintf-js": "^1.1.4",
    "@types/uniqid": "^5.3.4",
    "@types/webpack-bundle-analyzer": "^4.6.0",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "browserslist-config-erb": "^0.0.3",
    "chalk": "^4.1.2",
    "concurrently": "^8.2.1",
    "core-js": "^3.32.2",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "detect-port": "^1.5.1",
    "electron": "^26.2.1",
    "electron-builder": "^24.6.4",
    "electron-extension-installer": "^1.2.0",
    "electronmon": "^2.0.2",
    "eslint": "^8.49.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-erb": "^4.1.0-0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-import-resolver-webpack": "^0.13.7",
    "eslint-plugin-compat": "^4.2.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jest": "^27.4.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "mini-css-extract-plugin": "^2.7.6",
    "native-addon-loader": "^2.0.1",
    "prettier": "^3.0.3",
    "react-refresh": "^0.14.0",
    "react-test-renderer": "^18.2.0",
    "rimraf": "^5.0.1",
    "sass": "^1.67.0",
    "sass-loader": "^13.3.2",
    "style-loader": "^3.3.3",
    "terser-webpack-plugin": "^5.3.9",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "typescript": "^5.7.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.97.1",
    "webpack-bundle-analyzer": "^4.9.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.9.0"
  },
  "build": {
    "productName": "Oncoreport",
    "appId": "eu.dioncogen.oncoreport",
    "asar": true,
    "asarUnpack": "**\\*.{node,dll}",
    "files": [
      "dist",
      "node_modules",
      "package.json"
    ],
    "afterSign": ".erb/scripts/notarize.js",
    "mac": {
      "target": {
        "target": "default",
        "arch": [
          "arm64",
          "x64"
        ]
      },
      "type": "distribution",
      "hardenedRuntime": true,
      "entitlements": "assets/entitlements.mac.plist",
      "entitlementsInherit": "assets/entitlements.mac.plist",
      "gatekeeperAssess": false
    },
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "target": [
        "nsis"
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "tar.gz",
        "deb",
        "rpm"
      ],
      "category": "Science"
    },
    "directories": {
      "app": "release/app",
      "buildResources": "assets",
      "output": "release/build"
    },
    "extraResources": [
      "./assets/**"
    ],
    "publish": {
      "provider": "github",
      "owner": "knowmics-lab",
      "repo": "oncoreport"
    }
  },
  "devEngines": {
    "runtime": {
      "name": "node",
      "version": ">=14.x",
      "onFail": "error"
    },
    "packageManager": {
      "name": "npm",
      "version": ">=7.x",
      "onFail": "error"
    }
  },
  "electronmon": {
    "patterns": [
      "!**/**",
      "src/main/**"
    ],
    "logLevel": "quiet"
  }
}
